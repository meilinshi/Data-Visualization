class Item{
  String title;
  String itemtype;
  float dewey;
  int count;
  float x,y,z;

  Item(String title, String itemtype, float dewey, int count, float x, float y, float z){
    this.title = title;
    this.itemtype = itemtype;
    this.dewey = dewey;
    this.count = count;
    this.x = x;
    this.y = y;
    this.z = z;
  }
}
